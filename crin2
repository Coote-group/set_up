            $footer ="\n--Link1--\n\%chk=$inputfile_stripped.mp2opt.chk\n\#MP2(Full)/aug-cc-pVDZ guess=read geom=check Freq=noraman\n\n$inputfile_stripped MP2 frequencies\n\n$cm\n\n";
